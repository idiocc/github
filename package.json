{
  "name": "@idio/github",
  "version": "1.0.4",
  "description": "The GitHub OAuth Flow For Idio Web Server.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "doc -o README.md -n _idio -d -a",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 externs",
    "d1": "typal src -u",
    "externs": "typal types/externs/* -e",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "build",
    "src",
    "types"
  ],
  "externs": [
    "types/externs/user.js",
    "types/externs/externs.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/github.git"
  },
  "typedefs": "typedefs.json",
  "keywords": [
    "github",
    "idio"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/github/issues"
  },
  "homepage": "https://github.com/idiocc/github#readme",
  "devDependencies": {
    "@demimonde/dotenv": "^1.1.0",
    "@depack/render": "^1.3.4",
    "@idio/idio": "^1.1.2",
    "@typedefs/goa": "^0.1.5",
    "alamode": "^3.3.0",
    "documentary": "^1.35.9",
    "eslint-config-artdeco": "1.0.1",
    "typal": "^1.24.20",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "rqt": "^4.0.0"
  }
}
